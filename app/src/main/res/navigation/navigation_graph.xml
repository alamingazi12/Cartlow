    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/home_fragment"
    android:id="@+id/navigation_graph">

    <fragment
        android:id="@+id/home_fragment"
        android:name="com.example.cartflow.fragment.HomeFragment"
        android:label="first_fragments"
        tools:layout="@layout/fragment_home"/>

    <fragment
        android:id="@+id/categories_fragment"
        android:name="com.example.cartflow.fragment.CategoryFragment"
        android:label="second_fragments"
        tools:layout="@layout/fragment_category"/>

    <fragment
        android:id="@+id/profile_fragment"
        android:name="com.example.cartflow.fragment.ProfileFragment"
        android:label="thired_fragments"
        tools:layout="@layout/fragment_profile"/>

    <fragment
        android:id="@+id/cart_fragment"
        android:name="com.example.cartflow.fragment.CartFragment"
        android:label="fourth_fragments"
        tools:layout="@layout/fragment_cart"/>


    <fragment
        android:id="@+id/sell_fragment"
        android:name="com.example.cartflow.fragment.SellFragment"
        android:label="fifth_fragments"
